{
  "name": "zy-video-view",
  "private": false,
  "version": "1.1.7",
  "type": "module",
  "description": "A Vue 3 component library for video playback.",
  "keywords": [
    "vue",
    "video",
    "flv",
    "webrtc"
  ],
  "main": "./dist/zy-video-view.cjs.js",
  "module": "./dist/zy-video-view.es.js",
  "types": "./dist/zy-video-view.d.ts",
  "exports": {
    ".": {
      "types": "./dist/zy-video-view.d.ts",
      "import": "./dist/zy-video-view.es.js",
      "require": "./dist/zy-video-view.cjs.js",
      "default": "./dist/zy-video-view.es.js"
    },
    "./dist/style.css": "./dist/style.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist/*"
  ],
  "publishConfig": {
    "registry": "https://nexus.zhiyou-tec.com/repository/npm-host/"
  },
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview"
  },
  "peerDependencies": {
    "vue": "^3.0.0",
    "mpegts.js": "^1.7.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^22.9.1",
    "@vitejs/plugin-vue": "^5.2.0",
    "mpegts.js": "^1.7.3",
    "rollup-plugin-external-globals": "^0.12.1",
    "typescript": "~5.6.3",
    "unplugin-vue-define-options": "^1.5.2",
    "vite": "^5.4.11",
    "vite-plugin-dts": "^4.3.0",
    "vue": "^3.5.13",
    "vue-tsc": "^2.1.10"
  }
}
